<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="textGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#3B82F6;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#8B5CF6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#EC4899;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="#ffffff"/>
  
  <!-- Logo -->
  <g transform="translate(100, 200)">
    <!-- Central hub circle -->
    <circle cx="80" cy="80" r="50" fill="#8B5CF6" stroke="#ffffff" stroke-width="6"/>
    
    <!-- Top circle -->
    <circle cx="80" cy="20" r="25" fill="#8B5CF6" stroke="#ffffff" stroke-width="4"/>
    
    <!-- Left circle -->
    <circle cx="20" cy="80" r="25" fill="#3B82F6" stroke="#ffffff" stroke-width="4"/>
    
    <!-- Right circle -->
    <circle cx="140" cy="80" r="25" fill="#8B5CF6" stroke="#ffffff" stroke-width="4"/>
    
    <!-- Bottom circle -->
    <circle cx="80" cy="140" r="25" fill="#EC4899" stroke="#ffffff" stroke-width="4"/>
  </g>
  
  <!-- ClientFuse Text -->
  <text x="300" y="280" font-family="system-ui, -apple-system, sans-serif" font-size="120" font-weight="800" fill="url(#textGradient)">ClientFuse</text>
  
  <!-- Tagline -->
  <text x="300" y="350" font-family="system-ui, -apple-system, sans-serif" font-size="36" font-weight="400" fill="#64748B">Simplify Client Account Access</text>
  
  <!-- Subtitle -->
  <text x="300" y="400" font-family="system-ui, -apple-system, sans-serif" font-size="28" font-weight="300" fill="#94A3B8">For Marketing Agencies</text>
</svg>